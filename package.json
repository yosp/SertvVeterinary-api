{
  "name": "sertvveterinary-api",
  "version": "1.0.0",
  "description": "api manager for macro-project sertv",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava --require babel-register test/**-test.js",
    "pictures": "micro -p 5000 pictures.js",
    "users": "micro -p 5001 users.js",
    "auth": "micro -p 5002 auth.js"
  },
  "standard": {
    "ignore": [
    "/client.js",
    "/auth.js",
    "/internment.js",
    "/interecord.js",
    "/ethnicities.js",
    "/laboratory.js",
    "/race.js",
    "/pet.js",
    "/medicine.js"
    ],
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosp/SertvVeterinary-api.git"
  },
  "author": "Yeison Segura <yeisp1011@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yosp/SertvVeterinary-api/issues"
  },
  "homepage": "https://github.com/yosp/SertvVeterinary-api#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.16.3",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "standard": "^8.4.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "co": "^4.6.0",
    "gravatar": "^1.5.2",
    "sertvveterinary-db": "file:../sertvveterinary-db",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.1.9",
    "micro": "^6.1.0",
    "rethinkdb": "^2.3.3",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
